{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["dom", "es2020"],
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@pages/*": ["pages/*"],
      "@data/*": ["data/*"]
    }
  },
  "include": ["src"]
}
